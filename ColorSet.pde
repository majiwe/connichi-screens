class ColorSet {
   
  public color primary;
  public color secondary;
  public color third;
  public color background;
  public color highlight;
  public color elements;
  
  ColorSet( color p, color s, color t, color b, color h, color e){
    this.primary = p;
    this.secondary = s;
    this.third = t;
    this.background = b;
    this.highlight = h;
    this.elements = e;
  };
  
}